{
  "name": "@cubicler/cubicagentkit",
  "version": "1.0.2",
  "type": "module",
  "description": "A TypeScript SDK for creating CubicAgents that integrate seamlessly with the Cubicler AI orchestration framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm test",
    "preversion": "npm run test",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "cubicler",
    "ai",
    "agent",
    "express",
    "typescript",
    "sdk",
    "library",
    "nodejs",
    "api",
    "integration"
  ],
  "author": "hainayanda",
  "license": "Apache-2.0",
  "homepage": "https://github.com/hainayanda/CubicAgentKit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hainayanda/CubicAgentKit.git"
  },
  "bugs": {
    "url": "https://github.com/hainayanda/CubicAgentKit/issues"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "supertest": "^7.1.3",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
